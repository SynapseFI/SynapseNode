{
  "name": "synapsenode",
  "version": "2.0.0-alpha.1",
  "description": "Node.js Library for SynapseFI API v3.1 Rest",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --recursive --timeout 100000",
    "test-nodes": "mocha --recursive test/nodesTests/*.js --timeout 100000",
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "dist": "npm run clean && npm run build",
    "push": "npm run clean && npm run build && npm publish",
    "demo": "mocha -r ts-node/register src/demo/*.demo.ts --timeout 100000" 
  },
  "keywords": [
    "API",
    "payments",
    "synapsefi",
    "synapsepay"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.3",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "dotenv": "^16.0.3"
  }
}
